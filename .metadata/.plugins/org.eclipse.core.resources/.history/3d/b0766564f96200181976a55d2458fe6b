package Commands;

public class Buy implements Command {

	// ===========================================================================
	// INSTANCE VARIABLES
	// ===========================================================================
	
	private int itemQuantity;
	private int itemId;
	private int itemPrice;
	
	public Buy(int aItemQuantity, int aItemId) {
		itemQuantity = aItemQuantity;
		itemId = aItemId;
	}
	
	@Override
	public void command() {
		
	}

	@Override
	public boolean isRunnable() {
		return false;
	}
}
